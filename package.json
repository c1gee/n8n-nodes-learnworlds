{
  "name": "n8n-nodes-learnworlds",
  "version": "0.1.0",
  "description": "n8n nodes for LearnWorlds integration",
  "keywords": [
    "n8n-community-node-package",
    "learnworlds"
  ],
  "license": "MIT",
  "homepage": "https://github.com/yourusername/n8n-nodes-learnworlds#readme",
  "author": {
    "name": "Chris Gee",
    "email": "chris@b2bea.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/n8n-nodes-learnworlds.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/LearnWorldsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/LearnWorlds/LearnWorlds.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/request-promise-native": "~1.0.18",
    "@typescript-eslint/parser": "~5.45",
    "dotenv": "^16.5.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-core": "~0.125.0",
    "n8n-workflow": "~0.107.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.3.2",
    "typescript": "~4.8.4"
  },
  "bugs": {
    "url": "https://github.com/yourusername/n8n-nodes-learnworlds/issues"
  }
}
